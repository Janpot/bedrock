{
  "name": "bedrock",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "dev": "node index.js",
    "test": "node test.js --backend --frontend --hide",
    "test-backend": "node test.js --backend",
    "test-frontend": "node test.js --frontend",
    "test-frontend-hide": "node test.js --frontend --hide",
    "test-chrome": "node test.js --frontend --browser chrome",
    "test-chrome-hide": "node test.js --frontend --browser chrome --hide",
    "test-firefox": "node test.js --frontend --browser firefox",
    "test-firefox-hide": "node test.js --frontend --browser firefox --hide",
    "lint": "grunt jshint",
    "ci-test": "node test.js --backend --frontend",
    "ci-lint": "grunt jshint --mode=ci || exit 0",
    "coverage": "mkdir -p reports && rm -rf lib-cov; ./node_modules/visionmedia-jscoverage/jscoverage lib lib-cov && BEDROCK_COV=1 node test.js --backend > reports/coverage.html && rm -rf lib-cov",
    "bootstrap": "make -C ../bedrock-bootstrap bootstrap && rsync -av --exclude=js/bootstrap.min.js ../bedrock-bootstrap/bootstrap/ ./site/static/bootstrap/",
    "minify": "grunt",
    "install-protractor": "webdriver-manager update"
  },
  "dependencies": {
    "almond": "~0.2.6",
    "async": "~0.2.10",
    "bcrypt": "~0.7.2",
    "commander": "~1.3.0",
    "connect": "~2.7.10",
    "connect-mongo": "git://github.com/digitalbazaar/connect-mongo.git",
    "cors": "^2.3.1",
    "emailjs": "git://github.com/digitalbazaar/emailjs.git",
    "es6-promise": "=0.1.1",
    "express": "~3.3.5",
    "grunt": "~0.4.1",
    "grunt-angular-templates": "git://github.com/digitalbazaar/grunt-angular-templates",
    "grunt-cli": "~0.1.13",
    "grunt-contrib-cssmin": "~0.7.0",
    "grunt-contrib-requirejs": "~0.4.1",
    "http-proxy": "~1.0.2",
    "http-signature": "~0.10.0",
    "i18n": "~0.4.0",
    "json-schema": "git://github.com/digitalbazaar/json-schema.git",
    "jsonld": "^0.2.9",
    "mailparser": "~0.3.0",
    "mkdirp": "~0.3.5",
    "mongodb": "~1.3.18",
    "node-forge": "~0.2.32",
    "passport": "~0.1.9",
    "passport-local": "~0.1.3",
    "pkginfo": "~0.3.0",
    "prompt": "~0.2.11",
    "redback": "~0.3.6",
    "requirejs": "~2.1.8",
    "sprintf": "~0.1.1",
    "swig": "~1.3.2",
    "topcube": "~0.2.0",
    "ua-parser": "~0.3.0",
    "underscore": "~1.6.0",
    "ursa": "v0.8.0",
    "walk": "~2.2.1",
    "winston": "~0.7.0",
    "winston-mail": "~0.2.1"
  },
  "optionalDependencies": {
    "posix": "~1.0.0"
  },
  "devDependencies": {
    "chai": "~1.9.0",
    "chai-as-promised": "~4.1.0",
    "colors": "~0.6.2",
    "grunt-contrib-jshint": "^0.10.0",
    "mocha": "~1.17.1",
    "mocha-as-promised": "~2.0.0",
    "protractor": "^0.23.0",
    "temp": "^0.7.0",
    "visionmedia-jscoverage": "~1.0.0",
    "wd": "~0.2.10"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "directories": {
    "lib": "./lib/bedrock"
  },
  "main": "./lib/bedrock"
}
